{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Download Metadata",
			"cwd": "scripts",
			"request": "launch",
			"type": "dart",
			"program": "bin/download_metadata.dart"
		},
		{
			"name": "Download Extended Metadata Only",
			"cwd": "scripts",
			"request": "launch",
			"type": "dart",
			"program": "bin/download_metadata.dart",
			"args": [
				"--extended-only"
			],
			"codeLens": {
				"for": [
					"run-file",
					"debug-file"
				],
				"path": "scripts/bin/download_metadata.dart",
				"title": "${debugType} (extended-only)"
			}
		}
	]
}
